/data/KVManager/server/logs/gunicorn_access/gunicorn_access.log {
# 可能需要设置用户权限，如 su root root
daily
dateext
dateformat _%Y_%m_%d
dateyesterday
compress
rotate 30
missingok
notifempty
postrotate
        /usr/bin/killall -HUP rsyslogd
endscript

}
/data/KVManager/server/logs/gunicorn_error/gunicorn_error.log{
    # 也可能需要设置用户权限
    daily
    dateext
    dateformat _%Y_%m_%d
    dateyesterday
    rotate 30
    delaycompress
    missingok
    notifempty
    postrotate
        /usr/bin/killall -HUP rsyslogd
    endscript
}
