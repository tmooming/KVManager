/data/KVManager/server/logs/gunicorn_access/gunicorn_access.log {
su imagemgr imagemgr
daily
dateext
dateformat _%Y_%m_%d
dateyesterday
compress
rotate 30
missingok
notifempty
postrotate
        /usr/bin/killall -HUP rsyslogd
endscript

}
/data/KVManager/server/logs/gunicorn_error/gunicorn_error.log{
    su imagemgr imagemgr
    daily
    dateext
    dateformat _%Y_%m_%d
    dateyesterday
    rotate 30
    delaycompress
    missingok
    notifempty
    create 644 imagemgr imagemgr
    postrotate
        /usr/bin/killall -HUP rsyslogd
    endscript
}
